(macro N 100)

(setvar x 1)
(while (<(getvar x)(expand N))
  (if (=(%(getvar x)15)0)
    (syscall 1 1 (castint "FizzBuzz\n") 9)
  (if (=(%(getvar x)3)0)
    (syscall 1 1 (castint "Fizz\n") 5)
  (if (=(%(getvar x)5)0)
    (syscall 1 1 (castint "Buzz\n") 5)
    (print (getvar x))
  )))
  (chvar x (+(getvar x)1))
)
